{



"name": "字体很舒服(FontsKimochiIi) For Edge",
"version": "2016.09.05 | 07:36 | 31",
"author": "catcat520",
"description": "反馈 : http://bbs.kafan.cn/thread-1681393-1-1.html , 需附上网址 url , 截图 , 浏览器名称。",
"content_scripts": 
 [
   {
      "all_frames": true,
      "css": ["EdgeCustomMain.css"],
      "matches": ["<all_urls>"],
      "exclude_globs": ["*://*.jp/*","**://ja.*","**://jp.*"]
   },
   {
      "all_frames": true,
      "css": ["JpFonts.Domain.Sub.css"],
      "matches": ["<all_urls>"],
      "include_globs": ["**://*.jp*","**://ja.*","**://jp.*"]
   },
   {
      "all_frames": true,
      "css": ["JpFonts.Twitter.Sub.css"],
      "matches": ["*://twitter.com/*"]
   },
   {
      "all_frames": true,
      "css": ["JpFonts.Lang.Sub.css"],
      "matches": ["<all_urls>"],
      "exclude_globs": ["**://*.jp*","**://ja.*","**://jp.*"]
   }
 ]



}